<h2>🙌 社交</h2>
<table>
    <tr>
        <% socials.forEach(function(social){ %>
        <th><a href="<%= social.url %>" target="_blank"><%= social.name %></a></th>
        <% }); %>
    </tr>
    <tr>
        <% socials.forEach(function(social){ %>
        <td>
            <a href="<%= social.url %>" target="_blank">
                <img src="<%= social.icon %>" alt="<%= social.name %>">
            </a>
        </td>
        <% }); %>
    </tr>
</table>

<h2>📚 小书</h2>
<table>
    <thead align="center">
    <tr>
        <th>封面</th>
        <th>书名</th>
        <th>简述</th>
        <th>成就</th>
    </tr>
    </thead>

    <tbody align="left">
        <% books.forEach(function(book){ %>
        <tr>
            <th align="center">
                <img width="80" src="<%= book.cover %>" alt="<%= book.name %>">
            </th>
            <th>
                <a href="<%= book.url %>" target="_blank"><%= book.name %></a>
            </th>
            <th><%= book.desc %></th>
            <th>
                <% book.badges.forEach(function(badge){ %>
                    <img src="<%= badge %>" alt="badge">
                <% }); %>
            </th>
        </tr>
        <% }); %>
    </tbody>
</table>

<h2>💼 项目</h2>
<table>
    <thead align="center">
    <tr>
        <th>项目</th>
        <th>Github</th>
        <th>简述</th>
        <th>技术</th>
        <th>成就</th>
    </tr>
    </thead>

    <tbody align="left">
    <% projects.forEach(function(project){ %>
        <tr>
            <th>
                <a href="<%= project.preview || project.github %>" target="_blank">
                    <%= project.repo %>
                </a>
            </th>
            <th>
                <a href="<%= project.github %>" target="_blank">Github</a>
            </th>
            <th><%= project.logo %> <%= project.desc %></th>
            <th>
                <% project.techList.forEach(function(tech){ %>
                    <img src="<%= tech %>" alt="badge">
                <% }); %>
            </th>
            <th>
                <% project.badges.forEach(function(badge){ %>
                    <img src="<%= badge %>" alt="badge">
                <% }); %>
            </th>
        </tr>
    <% }); %>
    </tbody>
</table>

