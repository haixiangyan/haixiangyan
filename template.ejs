<h2>📖 文章</h2>
<ul>
    <% socials.forEach(function(social){ %>
    <li>
        <a target="_blank" href="<%= social.url %>"><%= social.name %></a>
    </li>
    <% }); %>
</ul>

<h2>📚 小书</h2>
<table>
    <thead align="center">
    <tr>
        <th>Logo</th>
        <th>书名</th>
        <th>描述</th>
        <th>成就</th>
    </tr>
    </thead>

    <tbody align="left">
    <% books.forEach(function(book){ %>
        <tr>
            <th><%= book.logo %></th>
            <th>
                <a href="<%= book.url %>" target="_blank"><%= book.name %></a>
            </th>
            <th><%= book.desc %></th>
            <th>
                <% book.badges.forEach(function(badge){ %>
                    <img src="<%= badge %>" alt="badge">
                <% }); %>
            </th>
        </tr>
    <% }); %>
    </tbody>
</table>

<h2>💼 项目</h2>
<table>
    <thead align="center">
    <tr>
        <th>Logo</th>
        <th>项目</th>
        <th>简述</th>
        <th>成就</th>
        <th>链接</th>
    </tr>
    </thead>

    <tbody align="left">
    <% projects.forEach(function(project){ %>
        <tr>
            <th><%= project.logo %></th>
            <th><%= project.repo %></th>
            <th><%= project.desc %></th>
            <th>
                <% project.badges.forEach(function(badge){ %>
                    <img src="<%= badge %>" alt="badge">
                <% }); %>
            </th>
            <th>
                <a href="<%= project.github %>">Github</a>
                <a href="<%= project.preview %>">预览</a>
            </th>
        </tr>
    <% }); %>
    </tbody>
</table>
